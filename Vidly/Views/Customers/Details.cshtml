@model Vidly.Models.Customer
@{
    ViewBag.Title = "Customer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Membership Type: @Model.MembershipType.Name</li>
    @if (Model.DateOfBirth.HasValue)
    {
        <li>Birthdate: @Model.DateOfBirth.Value</li>
    }
</ul>

